{
  "name": "@playkit-js/related",
  "version": "0.1.18",
  "main": "dist/playkit-related.js",
  "homepage": "https://github.com/kaltura/playkit-js-related#readme",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development",
    "docs:generate": "documentation build src/** -f md -o docs/api.md",
    "watch": "webpack --progress --color --watch --mode development",
    "test": "NODE_ENV=test karma start --color --mode development",
    "pushTaggedRelease": "git push --follow-tags --no-verify origin master",
    "release": "standard-version",
    "eslint": "eslint . --color",
    "prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "@playkit-js/ui-managers": "canary",
    "@playkit-js/kaltura-player-js": "canary",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "conventional-github-releaser": "3.1.3",
    "css-loader": "^6.5.1",
    "documentation": "^14.0.1",
    "eslint": "^8.4.0",
    "eslint-config-preact": "^1.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prettier": "^4.0.0",
    "humanize-duration": "^3.27.1",
    "prettier": "^2.5.1",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.4"
  },
  "peerDependencies": {
    "@playkit-js/kaltura-player-js": "canary"
  },
  "publishConfig": {
    "access": "public"
  },
  "kaltura": {
    "name": "related",
    "dependencies": {
      "playkit-ui-managers": "1.5.2"
    }
  },
  "browserslist": [
    "ie 11"
  ],
  "keywords": [
    "related entries grid",
    "kaltura",
    "player",
    "playkit-js",
    "playkit-js-related",
    "html5 player"
  ]
}
